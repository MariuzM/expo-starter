{
  "name": "expo-starter",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "d": "expo run:ios",
    "da": "expo run:android",
    "st": "expo start",
    "stcl": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "========== PRE BUILD ===========================": "",
    "preb": "expo prebuild",
    "prebcl": "expo prebuild --clean",
    "pod": "bunx pod-install",
    "========== PREVIEW BUILDS ======================": "",
    "bprevi": "eas build --profile preview --local -p ios",
    "bprevi-ni": "eas build --profile preview --local -p ios --non-interactive",
    "bpreva": "eas build --profile preview --local -p android",
    "bpreva-ni": "eas build --profile preview --local -p android --non-interactive",
    "========== PRODUCTION BUILDS ===================": "",
    "bprodi": "bun run preb && eas build --profile production --local -p ios",
    "bprodi-ni": "bun run preb && eas build --profile production --local -p ios --non-interactive",
    "bprodi-s": "bun run bprodi && bun run scripts/run.ts",
    "bprodi-s-ni": "bun run bprodi-ni && bun run scripts/run.ts",
    "========== FIX =================================": "",
    "doc": "bunx expo-doctor",
    "docfix": "bunx expo install --fix",
    "========== CLEAN ===============================": "",
    "clgradle": "cd android && ./gradlew clean && cd ..",
    "cl": "rm -rf .expo node_modules bun.lockb && bun install && bunx expo prebuild && bunx pod-install",
    "clf": "rm -rf .expo node_modules bun.lockb android ios && bun install && bunx expo prebuild && bunx pod-install",
    "========== UPDATE ==============================": "",
    "up": "ncu -u && bun install && bun docfix && bun clf"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-navigation/material-top-tabs": "^6.6.6",
    "@react-navigation/native": "^6.1.10",
    "ajv": "^8.12.0",
    "axios": "^1.6.7",
    "expo": "^50.0.7",
    "expo-blur": "~12.9.2",
    "expo-build-properties": "~0.11.1",
    "expo-constants": "~15.4.5",
    "expo-font": "^11.10.3",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.7",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-pager-view": "^6.2.3",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@faker-js/faker": "^8.4.1",
    "@types/react": "~18.2.56",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.3.3"
  }
}
