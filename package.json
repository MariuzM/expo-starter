{
  "name": "expo-starter",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "d": "expo run:ios",
    "st": "expo start",
    "stcl": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "========== PRE BUILD ===========================": "",
    "preb": "expo prebuild",
    "prebcl": "expo prebuild --clean",
    "pod": "bunx pod-install",
    "========== PREVIEW BUILDS ======================": "",
    "bp": "bunx expo prebuild && eas build --profile preview --local",
    "bpcl": "bun bp --clear-cache",
    "bpi": "eas build --profile preview --local -p ios",
    "bpa": "eas build --profile preview --local -p android",
    "========== FIX =================================": "",
    "doc": "bunx expo-doctor",
    "docfix": "bunx expo install --fix",
    "========== CLEAN ===============================": "",
    "clgradle": "cd android && ./gradlew clean && cd ..",
    "cl": "rm -rf .expo/ node_modules/ bun.lockb && bun install && bunx expo prebuild && bunx pod-install",
    "clf": "rm -rf .expo/ node_modules/ bun.lockb android/ ios/ && bun install && bunx expo prebuild && bunx pod-install",
    "========== UPDATE ==============================": "",
    "up": "ncu -u && bun install && bun docfix && bun clf"
  },
  "overrides": {
    "react-native-safe-area-context": "4.6.3"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "axios": "^1.6.2",
    "expo": "~49.0.20",
    "expo-blur": "~12.4.1",
    "expo-build-properties": "~0.8.3",
    "expo-constants": "~14.4.2",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.13",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.17",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.7.4",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@types/react": "~18.2.38",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "8.54.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "private": true
}
